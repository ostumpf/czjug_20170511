PERCENT_OP  : '%';
NEGATION    : '~';
RBRACK      : ']';
LBRACK      : '[';
PLUS        : '+';
MINUS       : '-';
SEMI        : ';';
POWER       : '^';
AT_OP       : '@';
AND_OP      : '&';
QUESTION    : '?';
