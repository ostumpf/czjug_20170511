// Comments
LINE_COMMENT            : '--' ~[\r\n]* -> skip;
HINT                    : '/*' WS_? '+' .*? '*/';
BLOCK_COMMENT           : '/*' (BLOCK_COMMENT | .)*? '*/' -> skip;

// Identifiers
ID          : LETTER_ (LETTER_ | DIGIT_ | '$')*;
QUOTED_ID   : '"' (~["] | '""' )* '"';

// Literals
NUMBER_LIT  : DIGIT_+
            | DIGIT_+ '.' DIGIT_*
            | (DIGIT_* '.' DIGIT_+)
            | (DIGIT_+ E_ ('+' | '-')? DIGIT_+)
            ;

STRING_LIT  : STANDARD_STRING_
            | DOLLAR_STRING_
            | UNICODE_STRING_
            | EXTENDED_STRING_
            | BIN_STRING_
            | HEX_STRING_
            ;

fragment UNICODE_ESC_       : '\\' HEX_DIGIT_ HEX_DIGIT_ HEX_DIGIT_ HEX_DIGIT_;
fragment ESC_               : '\\'.;
fragment DOUBLE_QUOTE_      : '\'\'';

fragment STRING_NEW_LINE_   : '\'' ([\u0000-\u0009\u000b\u000c\u000e-\u0020]* LINE_COMMENT? [\u000a\u000d] [\u0000-\u0009\u000b\u000c\u000e-\u0020]*)+ '\'';

fragment STANDARD_STRING_   : '\'' (~['] | DOUBLE_QUOTE_ | STRING_NEW_LINE_)* '\'';
fragment DOLLAR_STRING_     : '$$' .*? '$$';
fragment UNICODE_STRING_    : U_ AND_OP '\'' (~['\\] | '\\\\' | UNICODE_ESC_ | DOUBLE_QUOTE_ | STRING_NEW_LINE_)* '\'';
fragment EXTENDED_STRING_   : E_ '\'' (~['\\] | ESC_ | DOUBLE_QUOTE_ | STRING_NEW_LINE_)* '\'';
fragment BIN_STRING_        : B_ '\'' ([01] |  STRING_NEW_LINE_)* '\'';
fragment HEX_STRING_        : X_ '\'' (HEX_DIGIT_ | STRING_NEW_LINE_)* '\'';

DOUBLE_COLON : COLON COLON;

LIKE_OP        : NEGATION NEGATION;
LIKEB_OP       : NEGATION HASH_OP;
ILIKE_OP       : NEGATION NEGATION ASTERISK;
ILIKEB_OP      : NEGATION HASH_OP ASTERISK;
NOT_LIKE_OP    : EXCL LIKE_OP;
NOT_LIKEB_OP   : EXCL LIKEB_OP;
NOT_ILIKE_OP   : EXCL ILIKE_OP;
NOT_ILIKEB_OP  : EXCL ILIKEB_OP;

// Operators
OPERATOR_BIN   : '<=>' | '!=' | '<=' | '>=' | '||' | '<>' | '//' | '<<' | '>>';
OPERATOR_PRE   : '||/' | '|/' | '!!';

// Other
DOT         : '.';
WS          : WS_ -> skip;
LPAREN      : '(';
RPAREN      : ')';
COMMA       : ',';
ASTERISK    : '*';
COLON       : ':';
GT          : '>';
LT          : '<';
EQ          : '=';
EXCL        : '!';
PIPE        : '|';
HASH_OP     : '#';
